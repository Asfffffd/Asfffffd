[TimeLoop]
TEnd = 8.64e6 # [s]
DtInitial = 10 # [s]
MaxTimeStepSize = 1e4 # [s]

[Problem]
EnableGravity = true

[Grid]
File = ./grids/a3.msh
DomainMarkers = true # enable domain markers

[Matrix]
Problem.Name = matrix
Problem.BoundaryOverPressure = 5e5
Problem.BoundarySaturation = 0.5
SpatialParams.PermeabilityWell_ = 1e-12
SpatialParams.PermeabilityRock_ = 1e-18
SpatialParams.Porosity = 0.1
SpatialParams.VanGenuchtenAlpha = 2e-4
SpatialParams.VanGenuchtenN = 2.5
SpatialParams.Snr = 0.0
SpatialParams.Swr = 0.0

[Fracture]
Problem.Name = fractures
SpatialParams.Aperture = 1e-3
SpatialParams.Permeability = 1e-12
SpatialParams.PermeabilityBarrier = 1e-10
SpatialParams.Porosity = 1
SpatialParams.PorosityBarrier = 1
SpatialParams.VanGenuchtenAlpha = 1e-1
SpatialParams.VanGenuchtenN = 2
SpatialParams.Swr = 0.0
SpatialParams.Snr = 0.0
SpatialParams.Barrier.VanGenuchtenAlpha = 2e-4
SpatialParams.Barrier.VanGenuchtenN = 2.5
SpatialParams.Barrier.Snr = 0.0
SpatialParams.Barrier.Swr = 0.0

[Component]
SolidDensity = 2700
SolidThermalConductivity = 2.8
SolidHeatCapacity = 790
